        window.onbeforeunload = function () {
            if (document.cookie.split(';').some(elem => elem.indexOf("chat/session") > 0)) {
                if ($("body").has("div.chat-window").length) {
                    document.cookie = "chatIniciado=; expires=Thu, 01 Jan 1970 00:00:01 GMT;";
                }

                document.cookie.split(';').filter(function (e, d) {
                    if (e.indexOf("chat/session") > 0) {
                        var valor = parseInt(e.split("=")[1]) - 1;
                        if (valor == 0)
                            document.cookie = "chat/session=; expires=Thu, 01 Jan 1970 00:00:01 GMT;";
                        else
                            document.cookie = "chat/session=" + valor.toString() + ";expires=12/10/1900; path=/";
                    }
                });
            }

        }

        window.onload = function () {
            if (document.cookie.split(';').some(elem => elem.indexOf("chat/session") > 0)) {
                document.cookie.split(';').filter(function (e, d) {
                    if (e.indexOf("chat/session") > 0) {
                        var valor = parseInt(e.split("=")[1]) + 1;
                        document.cookie = "chat/session=" + valor.toString() + ";expires=12/10/1900; path=/";

                    }
                });
            }
        }


        if (!document.cookie.split(';').some(elem => elem.indexOf("chat/session") > 0) || !document.cookie.split(';').some(elem => elem.indexOf("chatIniciado") > 0)) { // si no esta presnte chat/session o si no esta presente el chat
            $(function () {

                var ip_publico;

                $("#<%=Me.hfip.Value%>").val(ip_publico);

                $.chat({

                    user: {
                        Id: '<%= Me.UserId%>',
                        Name: '<%= Me.UserName%>',
                        Foto: '<%= Me.UserProfilePictureUrl%>'
                    },
                    titleText: 'Chat Interno',

                    typingText: ' escribiendo...',

                    emptyRoomText: "0 usuarios conectados!!!",

                    adapter: new SignalRAdapter()
                });

                if (!document.cookie.split(';').some(elem => elem.indexOf("chat/session") > 0)) {
                    document.cookie = "chat/session=" + 0 + "; path=/";
                }
                document.cookie = "chatIniciado=" + 1 + "; path=/";
                
                var hub = $.connection.nomadeHub;
                var $actionCS = $("#actionCS");
                var $actionCM = $("#actionCM");
                var $actionPA = $("#actionPA");
                var $actinoNO = $(".nomade-notif");
                var $actionALL = $("#notifAll");

                var userActual = $("#ctl00_txtus").val();
                var sesionId = $("#ctl00_hdsesion").val();

                // notificacion otro logeo con el mismo usuario
                hub.client.verificar = function (usuario, session, empresa) {
                    var key = localStorage.getItem("key" + usuario);
                    if (key == null) {
                        if (userActual == usuario) {
                            if (sesionId == session) { } else {
                                $("#ctrlEmpresa").html(empresa);
                                $("#ctrlUsuario").html(usuario);
                                document.cookie = "otraSesion=" + empresa + "|" + usuario + ";expires=12/10/1900; path=/";
                                $("#ventanaW").modal("show");
                            }
                        }
                    }
                }

                hub.client.cerrarActual = function (usuario, session) {
                    if (userActual == usuario && sesionId == session) {
                        cerrar_sesion();
                    }
                }

                hub.client.cerrarOtra = function (usuario, session) {
                    if (userActual == usuario && sesionId == session) {
                    } else {
                        cerrar_sesion();
                    }
                }

                hub.client.lanzar = function () {
                    cerrar_sesion();
                }

                hub.client.paralelo = function (key, usuario) {
                    localStorage.setItem("key" + usuario, key);
                }

                //Notificaciones              

                hub.client.notificar = function (numNotif, linkNotif, iconoNotif, textoNotif, fechaNotif, tipoNotif, usuario, titulo, codReferencia) {

                    if (usuario != userActual) {
                        $('.countNotif').text(numNotif);

                        $('#header_notification_bar').pulsate({ color: "#66bce6", repeat: 2 });

                        var newNotif = '';

                        newNotif += '<li >'
                        newNotif += '<a data-link="' + linkNotif + '" id="' + codReferencia + '" class="nomade-notif" href="#">'
                        newNotif += '<span class="label label-' + tipoNotif + '" style="width:13px;height:13px;"><i class="' + iconoNotif + '" style="line-height:1;"></i></span>'
                        newNotif += textoNotif
                        newNotif += '<span class="time">&nbsp' + fechaNotif + '</span>'
                        newNotif += '</a>'
                        newNotif += '</li>'

                        if ($('#conteNotif li').length == 0) {
                            $('#conteNotif').append(newNotif);
                        } else {
                            $($('#conteNotif li')[0]).before(newNotif);
                        }

                        $.gritter.add({
                            title: titulo,
                            text: '<a data-link="' + linkNotif + '" id="' + codReferencia + '" class="nomade-notif">' + textoNotif + '</a>',
                            sticky: false,
                            time: '',
                            class_name: 'gritter-light'
                        });

                        $(".nomade-notif").on("click", function (e) {
                            e.preventDefault();
                            var link = $(this).attr("data-link");
                            var codReferencia = $(this).attr("id");
                            hub.server.actualizarNotificacion(userActual, codReferencia);
                            location.href = link;
                        });
                    }
                }

                hub.on("dibujarNotificacionesAll", function (notif) {
                    $('#contentAllNotificaciones').html('');

                    if (notif.length == 0) {
                        $('#contentAllNotificaciones').append('<div class="alert alert-info"><strong>Info!</strong> Usted no tiene notificaciones.</div>');
                        $("#ventanaN").modal('show');
                    }

                    var jsonAlertas = '';
                    for (var i = 0; i < notif.length; i++) {
                        if (notif[0].UserDestino == userActual) {
                            var newNotif = '';

                            var classV = notif[i].Visto == 0 ? 'important' : 'success';

                            newNotif += '<tr>';
                            newNotif += '<td style="vertical-align:middle" class="highlight"><div class="' + classV + '"></div><a data-link="' + notif[i].Link + '" id="' + notif[i].CodReferencia + '" class="nomade-notif" href="#">' + notif[i].UserOrigen + '</a></td>'
                            newNotif += '<td class="">' + notif[i].Fecha + '</td>';
                            newNotif += '<td style="vertical-align:middle" class="">' + notif[i].Modulo + '</td>';
                            newNotif += '<td class="">' + notif[i].Texto + '</td>';
                            newNotif += '</tr>';

                            $('#contentAllNotificaciones').append(newNotif);
                            $("#ventanaN").modal('show');

                            jsonAlertas += '{';
                            jsonAlertas += '"USUARIO" : "' + notif[i].UserOrigen + '",';
                            jsonAlertas += '"FECHA" : "' + notif[i].Fecha + '",';
                            jsonAlertas += '"MODULO" : "' + notif[i].Modulo + '",';
                            jsonAlertas += '"MENSAJE" : "' + notif[i].Texto + '",';
                            jsonAlertas += '"COD_REFERENCIA" : "' + notif[i].CodReferencia + '",';
                            jsonAlertas += '"LINK" : "' + notif[i].Link + '"';
                            jsonAlertas += '},';
                        }
                    }

                    oTableNotificaciones2.fnClearTable();
                    if (jsonAlertas !== "") {
                        jsonAlertas = jsonAlertas.slice(0, -1);
                        jsonAlertas = '[' + jsonAlertas + ']';

                        oTableNotificaciones2.fnAddData(jQuery.parseJSON(jsonAlertas));
                        setTimeout(function () {
                            oTableNotificaciones2.fnAdjustColumnSizing();
                        }, 300);
                    }
                    //else {
                    //    jsonAlertas = '[{}]';
                    //}
                    

                    $(".nomade-notif").on("click", function (e) {
                        e.preventDefault();
                        var link = $(this).attr("data-link");
                        var codReferencia = $(this).attr("id");
                        hub.server.actualizarNotificacion(userActual, codReferencia);
                        location.href = link;
                    });

                });

                hub.on("dibujarNotificaciones", function (notif) {
                    for (var i = 0; i < notif.length; i++) {
                        if (notif[0].UserDestino == userActual) {
                            var newNotif = '';

                            newNotif += '<li>';
                            newNotif += '<a data-link="' + notif[i].Link + '" id="' + notif[i].CodReferencia + '" class="nomade-notif" href="#">';
                            newNotif += '<span class="label label-' + notif[i].Tipo + '" style="width:13px;height:13px;"><i class="' + notif[i].Icono + '" style="line-height:1;"></i></span>';
                            newNotif += notif[i].Texto;
                            newNotif += '<span class="time">&nbsp' + notif[i].Fecha + '</span>';
                            newNotif += '</a>';
                            newNotif += '</li>';

                            if ($('#conteNotif li').length == 0) {
                                $('#conteNotif').append(newNotif);
                            } else {
                                $($('#conteNotif li')[0]).before(newNotif);
                            }

                            if (i + 1 == notif.length) {
                                $($('#conteNotif li')[0]).before("<li><p>Usted tiene <span class='countNotif'></span> Notificaciones</p></li>");
                                $('.countNotif').text(i + 1);
                            }
                        }
                    }

                    $(".nomade-notif").on("click", function (e) {
                        e.preventDefault();
                        var link = $(this).attr("data-link");
                        var codReferencia = $(this).attr("id");
                        hub.server.actualizarNotificacion(userActual, codReferencia);
                        location.href = link;
                    });
                });

                $.connection.hub.start().done(function () {
                    hub.server.verificaSesion(sesionId, $("#<%=Me.sHddctlg.ClientID%>").val());

                    hub.server.getNotificacionHistory(userActual);

                    $actionCS.on("click", function () {
                        hub.server.cerrarOtraSesion(sesionId);
                        document.cookie = "otraSesion=; expires=Thu, 01 Jan 1970 00:00:01 GMT;";
                        $("#ventanaW").modal("hide");
                    });
                    $actionCM.on("click", function () {
                        document.cookie = "otraSesion=; expires=Thu, 01 Jan 1970 00:00:01 GMT;";
                        hub.server.cerrarSesionActual(sesionId);
                    });
                    $actionPA.on("click", function () {
                        hub.server.trabajarParalelo();
                        document.cookie = "otraSesion=; expires=Thu, 01 Jan 1970 00:00:01 GMT;";
                        $("#ventanaW").modal("hide");
                    });
                    $actionALL.on("click", function (e) {
                        e.preventDefault();
                        hub.server.getNotificacionAll(userActual);
                    });
                });
            });
        } else {
            window.onmousemove = function () {
                if (!document.cookie.split(';').some(elem => elem.indexOf("chatIniciado") > 0)) {
                    window.onmousemove = null;
                    location.reload();

                }

                var verificaOtraSesion = document.cookie.split(';').some(elem => elem.indexOf("otraSesion") > 0);

                if (verificaOtraSesion) {

                    var $actionCS = $("#actionCS");
                    var $actionCM = $("#actionCM");
                    var $actionPA = $("#actionPA");

                    document.cookie.split(';').filter(function (e, d) {
                        if (e.indexOf("otraSesion") > 0 && !document.cookie.split(';').some(elem => elem.indexOf("opcionOtraSesion") > 0)) {
                            var empresa = e.split("=")[1].split("|")[0];
                            var usuario = e.split("=")[1].split("|")[1];
                            $("#ctrlEmpresa").html(empresa);
                            $("#ctrlUsuario").html(usuario);
                            $("#ventanaW").modal("show");
                        }
                    });

                    $actionCS.click(function () {
                        document.cookie = "opcionOtraSesion=" + "CS" + ";expires=12/10/1900; path=/";
                        $("#ventanaW").modal("hide");
                    });
                    $actionCM.click(function () {
                        document.cookie = "opcionOtraSesion=" + "CM" + ";expires=12/10/1900; path=/";
                        $("#ventanaW").modal("hide");
                    });
                    $actionPA.click(function () {
                        document.cookie = "opcionOtraSesion=" + "PA" + ";expires=12/10/1900; path=/";
                        $("#ventanaW").modal("hide");
                    });


                } else { $("#ventanaW").modal("hide"); }
            };
        }

