        //configuracion inputmask 
        $.extend($.inputmask.defaults.definitions, {
            'A': { //letras sin espacios ni tildes
                validator: "[A-Za-zÑñ]",
                cardinality: 1,
                casing: "upper"

            },
            'D': { //DESCRIPCION
                validator: "[A-Za-zñÑáéíóúÁÉÍÓÚ0-9:,;./\(\)\#--- ]",
                cardinality: 1,
                casing: "upper"

            },
            'F': { //FLOAT - DECIMAL
                validator: "[0-9,]",
                cardinality: 1,
                casing: "upper"

            },
            'Z': { //letras con espacio sin tilde y &
                validator: "[A-Za-zñÑáéíóúÁÉÍÓÚ\&/0-9\-. ]",
                cardinality: 1,
                casing: "upper"

            },
            'k': { // letras con espacio
                validator: "[A-Za-zÑñáéíóúÁÉÍÓÚ. ]",
                cardinality: 1,
                casing: "upper"

            },
            'L': { // letras con espacio
                validator: "[A-Za-zÑñáéíóúÁÉÍÓÚ/\(\). ]",
                cardinality: 1,
                casing: "upper"

            },
            'S': {//simbolos para moneda
                validator: "[A-Za-zÑñ $\/.&#8364;]",
                cardinality: 1


            },
            '#': {//alfanumericos sin espacio
                validator: "[A-Za-zÑñ0-9]",
                cardinality: 1,
                casing: "upper"
            },
            '%': { //letras con tilde numeros y espacio y guion bajo
                validator: "[A-Za-zñÑáéíóúÁÉÍÓÚ0-9/_.\(\) ]",
                cardinality: 1,
                casing: "upper"
            },
            'T': {
                validator: "[0-9 #*---/]",
                cardinality: 1,
                casing: "upper"
            },
            'E': {
                validator: "[A-Za-zñÑ0-9@._ ]",
                cardinality: 1,
                casing: "upper"
            },
            'M': {
                validator: "[0-9()+- ]",
                cardinality: 1
            },
            '!': {
                validator: "[A-Za-zñÑáéíóúÁÉÍÓÚ0-9.,--- ]",
                cardinality: 1,
                casing: "upper"
            },
            'W': {
                validator: "[A-Za-zÑñ0-9:/.\~_\-]",
                cardinality: 1,
                casing: "upper"
            },
            'R': {
                validator: "[A-Za-zñÑáéíóúÁÉÍÓÚ0-9:,;.\(\)\#--- ]",
                cardinality: 1,
                casing: "upper"
            },
            'U': {
                validator: "[A-Za-zÑñ0-9 ]",
                cardinality: 1,
                casing: "upper"
            },
            'C': {
                validator: "[0-9---]",
                cardinality: 1,
                casing: "upper"
            },
            'P': {
                validator: "[0-9A-Za-zñÑáéíóúÁÉÍÓÚ \-]",
                cardinality: 1,
                casing: "upper"
            },
            //USUARIO
            'O': {
                validator: "[A-Za-zÑñ0-9---_]",
                cardinality: 1,
                casing: "upper"
            },
            'H': { //horas personalizadas
                validator: "0[0-9]|1[0-9]|2[0-3]",
                cardinality: 2,
                prevalidator: [
                    { validator: "[012]", cardinality: 1 }
                ]
            },
            '0': { //tiempo 00 al 60
                validator: "[0-5][0-9]",
                cardinality: 2,
                prevalidator: [
                    { validator: "[0-5]", cardinality: 1 }
                ]
            },

            '&': { //basic year
                validator: "20\\d{2}|19[5-9]\\d",
                cardinality: 4,
                prevalidator: [
                    { validator: "[12]", cardinality: 1 },
                    { validator: "(19|20)", cardinality: 2 },
                    { validator: "(20\\d|19[5-9])", cardinality: 3 }
                ]
            },
            '@': { //basic year
                validator: "20\\d{2}|19[0-9]\\d",
                cardinality: 4,
                prevalidator: [
                    { validator: "[12]", cardinality: 1 },
                    { validator: "(19|20)", cardinality: 2 },
                    { validator: "(20\\d|19[0-9])", cardinality: 3 }
                ]
            }
            , 'l': { // letras con espacio
                validator: "[A-Za-zÑñáéíóúÁÉÍÓÚ ]",
                cardinality: 1
            }
        });

        function cerrar_sesion() {
            location.href = 'cerrar_sesion.aspx';
        }

        function EnviarPagina() {
